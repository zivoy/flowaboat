{
    "name": "flowabot",
    "description": "Modular Discord bot with fun features including twitch commands and advanced osu! commands.",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "npm stop; sh ./update.sh; ACCEPT_HIGHCHARTS_LICENSE=1 npm i && [ ! -f ./config.json ] && npm run config; forever start -a -l /home/ziv132/bot/logs/log.log -o /home/ziv132/bot/logs/out.log -e /home/ziv132/bot/logs/err.log --minUptime 1000 --spinSleepTime 1000 index.js; forever start --minUptime 1000 --spinSleepTime 1000 tail.js ./logs",
        "test": "echo \"Error: no test specified\" && exit 1",
        "commands": "node generate-commands-md",
        "config": "node generate-config",
        "emojis": "node upload-emojis",
		"stop": "sh stop.sh",
		"restart": "forever restartall"
    },
    "author": "LeaPhant",
    "repository": {
        "type": "git",
        "url": "https://github.com/LeaPhant/flowabot.git"
    },
    "license": "MIT",
    "dependencies": {
        "axios": "0.18.1",
        "canvas": "^2.5.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.2",
        "discord.js": "^11.5.1",
        "diskusage": "^1.1.2",
        "ffmpeg-static": "^2.4.0",
        "forever": "1.0.0",
        "fs-extra": "^8.1.0",
        "highcharts-export-server": "^2.0.24",
        "jimp": "^0.6.0",
        "lzma": "^2.3.2",
        "mathjs": "^6.0.2",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.2.2",
        "node-emoji": "^1.8.1",
        "node-localstorage": "^1.3.0",
        "object-path": "^0.11.4",
        "ojsama": "^1.2.1",
        "osu-parser": "git+https://github.com/LeaPhant/osu-parser.git",
        "readline-sync": "^1.4.9",
        "semver": "^6.2.0",
        "unzip": "^0.1.11",
        "vm2": "^3.6.10",
		"express": "4.15.2"
    }
}
